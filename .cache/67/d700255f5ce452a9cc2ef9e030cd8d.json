   position: absolute;\r\n    border-radius: 50%;\r\n}\r\n#uiContainer {\r\n    position: absolute; top: 0; left: 0; width: 100vw; height: 100vh; text-align: center;\r\n}\r\np { margin: 0.4em; }\r\nh1 { \r\n    color: var(--c1); \r\n    text-shadow: 5px 5px 5px black, 0 0 0.5em var(--c1);\r\n}\r\n#infoText {\r\n    text-shadow: 2px 2px 3px var(--c2);\r\n}\r\n#gameContainer {\r\n    padding: 0 1em;\r\n    border-radius: 0.5em;\r\n    max-width: 50vw;\r\n    min-width: 20em;\r\n    background-image: linear-gradient(var(--c1),var(--c4));\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n    opacity: 0.8;\r\n    box-shadow: 10px 10px 10px black;\r\n    text-align: center;\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\ntable {\r\n    border-collapse: collapse;\r\n    margin: 0 4em;\r\n}\r\n.cell { \r\n    padding: 1em; \r\n    margin: 0; \r\n    width: 10vw;\r\n    height: 5vh;\r\n    transition: box-shadow 0.5s linear;\r\n    text-shadow: 2px 2px 3px var(--c2);\r\n    font-weight: 900;\r\n    font-size: 1.1em;\r\n}\r\n.cell:hover:not(.filled) {\r\n    box-shadow: inset 0 0 5px 1px white;\r\n    cursor: pointer;\r\n}\r\n#cell2, #cell5, #cell8 {\r\n    border-left: 1px solid white;\r\n    border-right: 1px solid white;\r\n}\r\n#cell4, #cell5, #cell6 {\r\n    border-top: 1px solid white;\r\n    border-bottom: 1px solid white;\r\n}\r\nbutton {\r\n    outline: none;\r\n    border: 1px solid var(--c2);\r\n    border-radius: 30%;\r\n    background-color: hsl(0,0%,0%,0);\r\n    color: #ccc;\r\n    padding: 0.7em;\r\n    transition: box-shadow 0.5s linear, color 0.5s linear, background-color 0.5s linear;\r\n}\r\nbutton:not(.selected) {\r\n    border: 1px dashed var(--c2);\r\n}\r\nbutton:hover:not(.selected) {\r\n    box-shadow: inset 0 0 5px 0 var(--c1);\r\n    cursor: pointer;\r\n    color: var(--c0);\r\n}\r\nbutton:active:not(.selected) {\r\n    background-color: var(--c3);\r\n    box-shadow: inset 0 0 5px 1px var(--c3);\r\n    border: 1px solid var(--c2);\r\n}\r\nbutton.selected {\r\n    background-color: var(--c3);\r\n    box-shadow:  inset 0 0 5px 0 var(--c1);\r\n    color: var(--c0);\r\n}\r\nbutton.newGame {\r\n    border: none;\r\n    box-shadow: 0 0 5px 1px var(--c2);\r\n    color: var(--c0);\r\n    transition: background-color 0.5s linear, box-shadow 0.5s linear;\r\n}\r\nbutton.newGame:hover {\r\n    box-shadow: 0 0 10px 2px var(--c0);\r\n}\r\nbutton.newGame:active {\r\n    background-color: var(--c3);\r\n    border: none;\r\n    box-shadow: 0 0 5px 1px var(--c0);\r\n}\r\n\r\n#planetContainer {\r\n    position: absolute;\r\n    height: 200vw;\r\n    top: 50%;\r\n    animation: spin 60s linear infinite;\r\n}\r\n.planet { \r\n    width: 200px;\r\n    height: 200px;\r\n    border-radius: 50%;\r\n    box-shadow:\r\n      inset 0 0 50px #fff,\r\n      inset 20px 0 80px #f0f,\r\n      inset -20px 0 80px #0ff,\r\n      inset 20px 0 300px #f0f,\r\n      inset -20px 0 300px #0ff,\r\n      0 0 10px #fff,\r\n      -10px 0 80px #f0f,\r\n      10px 0 80px #0ff;\r\n}\r\n@keyframes spin {\r\n    100% { transform: rotate(360deg); }\r\n}\r\n\r\na { text-decoration: none; }\r\na:visited { color: var(--c0); }\r\na:hover { color: var(--c1); }\r\na:active { color: var(--c1); }"},"lineCount":null}},"error":null,"hash":"6d5f87b8a5fe53349f3b95e80e9c6a5f","cacheData":{"env":{}}}